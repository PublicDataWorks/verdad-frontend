    "prepare": "husky install",
    "nyc": "nyc",
    "coverage": "npm run test:unit:coverage && npm run test:e2e:run && node ./merge-cov.cjs",
    "run-eslint": "eslint --cache --fix --ignore-path .gitignore --ext .ts,.tsx ."
  },
  "nyc": {
    "report-dir": "cypress-coverage"
  },
  "dependencies": {
