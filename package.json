    "@typescript-eslint/parser": "6.17.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-istanbul": "^0.34.6",
    "cypress": "^13.6.1",
    "eslint": "^8.53.0",
